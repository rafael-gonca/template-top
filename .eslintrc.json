{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
    "rules": {
        "indent": "off",
        "prettier/prettier": ["error", { "tabWidth": 4, "useTabs": false }]
    }
}
